{
    "name": "presttec\/laravel-cashier",
    "description": "Laravel Cashier provides an expressive, fluent interface to Stripe's subscription billing services.",
    "keywords": [
        "laravel",
        "stripe",
        "billing"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-cashier\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-cashier"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "presttec\/illuminate-consoleilluminate-console": "^10.0|^11.0",
        "presttec\/illuminate-contracts": "^10.0|^11.0",
        "presttec\/illuminate-database": "^10.0|^11.0",
        "presttec\/illuminate-http": "^10.0|^11.0",
        "presttec\/illuminate-log": "^10.0|^11.0",
        "presttec\/illuminate-notifications": "^10.0|^11.0",
        "presttec\/illuminate-pagination": "^10.0|^11.0",
        "presttec\/illuminate-routing": "^10.0|^11.0",
        "presttec\/illuminate-support": "^10.0|^11.0",
        "presttec\/illuminate-view": "^10.0|^11.0",
        "presttec\/money": "^4.0",
        "presttec\/carbon": "^2.0|^3.0",
        "presttec\/stripe-php": "^13.0",
        "presttec\/consolesymfony-console": "^6.0|^7.0",
        "presttec\/http-kernel": "^6.0|^7.0",
        "presttec\/polyfill-intl-icu": "^1.22.1"
    },
    "require-dev": {
        "presttec\/dompdf": "^2.0",
        "presttec\/mockery": "^1.0",
        "presttec\/testbench": "^8.18|^9.0",
        "presttec\/phpstan": "^1.10",
        "presttec\/phpunit": "^10.4"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values.",
        "dompdf\/dompdf": "Required when generating and downloading invoice PDF's using Dompdf (^1.0.1|^2.0)."
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src\/",
            "Laravel\\Cashier\\Database\\Factories\\": "database\/factories\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "15.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/cashier": "*"
    },
    "conflicts": {
        "laravel\/cashier": "*"
    }
}