{
    "name": "presttec\/laravel-cashier",
    "description": "Laravel Cashier provides an expressive, fluent interface to Stripe's subscription billing services.",
    "keywords": [
        "laravel",
        "stripe",
        "billing"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-cashier\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-cashier"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "presttec\/dompdf": "^0.8.0",
        "presttec\/illuminate-contracts": "~5.8.0|^6.0|^7.0",
        "presttec\/illuminate-database": "~5.8.0|^6.0|^7.0",
        "presttec\/illuminate-http": "~5.8.0|^6.0|^7.0",
        "presttec\/illuminate-log": "~5.8.0|^6.0|^7.0",
        "presttec\/illuminate-notifications": "~5.8.0|^6.0|^7.0",
        "presttec\/illuminate-routing": "~5.8.0|^6.0|^7.0",
        "presttec\/illuminate-support": "~5.8.0|^6.0|^7.0",
        "presttec\/illuminate-view": "~5.8.0|^6.0|^7.0",
        "presttec\/money": "^3.2",
        "presttec\/carbon": "^2.0",
        "presttec\/stripe-php": "^7.0",
        "presttec\/http-kernel": "^4.3",
        "presttec\/intl": "^4.3"
    },
    "require-dev": {
        "presttec\/mockery": "^1.0",
        "presttec\/testbench": "^3.8|^4.0|^5.0",
        "presttec\/phpunit": "^7.5|^8.0"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/cashier": "*"
    },
    "conflicts": {
        "laravel\/cashier": "*"
    }
}