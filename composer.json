{
    "name": "presttec\/laravel-cashier",
    "description": "Laravel Cashier provides an expressive, fluent interface to Stripe's subscription billing services.",
    "keywords": [
        "laravel",
        "stripe",
        "billing"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-cashier\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-cashier"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "presttec\/dompdf": "^1.2.1",
        "presttec\/illuminate-consoleilluminate-console": "^8.37|^9.0",
        "presttec\/illuminate-contracts": "^8.37|^9.0",
        "presttec\/illuminate-database": "^8.37|^9.0",
        "presttec\/illuminate-http": "^8.37|^9.0",
        "presttec\/illuminate-log": "^8.37|^9.0",
        "presttec\/illuminate-notifications": "^8.37|^9.0",
        "presttec\/illuminate-routing": "^8.37|^9.0",
        "presttec\/illuminate-support": "^8.37|^9.0",
        "presttec\/illuminate-view": "^8.37|^9.0",
        "presttec\/money": "^3.2|^4.0",
        "presttec\/carbon": "^2.0",
        "presttec\/stripe-php": "^7.39|^8.0",
        "presttec\/http-kernel": "^5.0|^6.0",
        "presttec\/polyfill-intl-icu": "^1.22.1"
    },
    "require-dev": {
        "presttec\/mockery": "^1.0",
        "presttec\/testbench": "^6.0|^7.0",
        "presttec\/phpunit": "^9.0"
    },
    "suggest": {
        "ext-intl": "Allows for more locales besides the default \"en\" when formatting money values."
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src\/",
            "Laravel\\Cashier\\Database\\Factories\\": "database\/factories\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "13.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/cashier": "*"
    },
    "conflicts": {
        "laravel\/cashier": "*"
    }
}