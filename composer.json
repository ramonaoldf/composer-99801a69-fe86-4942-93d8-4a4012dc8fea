{
    "name": "presttec\/laravel-cashier",
    "description": "Laravel Cashier provides an expressive, fluent interface to Stripe's subscription billing services.",
    "keywords": [
        "laravel",
        "stripe",
        "billing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "presttec\/illuminate-filesystem": "~5.0",
        "presttec\/illuminate-support": "~5.0",
        "presttec\/carbon": "~1.0",
        "presttec\/stripe-php": "~1.9",
        "presttec\/http-kernel": "~2.3"
    },
    "require-dev": {
        "presttec\/illuminate-routing": "~5.0",
        "presttec\/illuminate-view": "~5.0",
        "presttec\/mockery": "~0.9",
        "presttec\/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Laravel\\Cashier\\": "src\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "replaces": {
        "laravel\/cashier": "*"
    },
    "conflicts": {
        "laravel\/cashier": "*"
    }
}